Program Ex3GradeConvert;
   
Var grade : Integer;
	gradeChar : Char;

Begin
case 10:
  switch grade 
  begin
  	case 10:
  	case 9:
  		gradeChar:=A;
  		break;
  	case 8:
  	case 7:
  		gradeChar:=B;
  		break;
  	case 6:
  	case 5:
  		gradeChar:=C;
  		break;
  	case 4:
  	case 3:
  		gradeChar:=D;
  		break;
  	case 2:
  	case 1:
  		gradeChar:=F;
  		break;
	default:
		gradeChar:=F;
		break;
  end;
end.
