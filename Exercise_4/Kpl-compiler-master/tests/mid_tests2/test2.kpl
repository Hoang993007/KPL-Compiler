program bai2;
var x:integer;
    y:char;
begin
    switch x
    begin
        case 0<x<4:
            y := 'F';
            break;
        case 4<=x<5:
            y:= 'D';
            break;
        case 5<=x<7:
            y:='C';
            break;
        case 7<=x<8:
            y:='B';
            break;
        case 8<=x<=10:
            y:='A';
            break;
        default:
            y:='';
    end;